.input{
  height: 100%;
  display:flex;
  flex-direction: column;
  background-color: #ffffff;
}
.block,.inter{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-top: 2px solid #dbdbdb;
  padding-left: 5%;
  height: 60px;
}
.block1{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  border-bottom: 1px solid #dbdbdb;
  padding-left: 5%;
  padding-top:3%;
  height:174px;
}
.ip, .ph{
  font-size: 18px;
  width:265px;
  height: 100%;
  text-align: right;
}
.ip1{
  padding-top: 10px;
  width:283px;
}
.title ,.description ,.text1, .text2, .text3{
  font-size:18px;
  left: 20px;
  right:10px;
  width:100px;
}

.date, .time{
  text-align: right;
  width:132px;
  color: #707070;
}
.inter{
  font-size:16px;
  color:#8a8a8a;
  background-color: #f0f0f0;
  vertical-align: center;
  padding-left: 5%;
  padding-bottom: 0px;
  height: 45px;
}
.icon{
  height:25px;
  width:25px;
  padding-right: 15px;
}
.buttons{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 5%;
  align-items: center;
  width:385px;
}
.button{
  border: 3px solid #c8c8c8;
  border-radius: 10px;
  height: 40px;
  width: 170px;
  color: #989898;
  display: flex;
  align-items: center;
  justify-content: center;
}